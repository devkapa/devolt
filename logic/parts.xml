    <part type="ic" uid="ic-74hc00">
        <name>74HC00</name>
        <desc>This chip has quad 2-input NAND gates.</desc>
        <picture>ic.png</picture>
        <texture>ic.png</texture>
        <icConfig>
            <dipCount>14</dipCount>
            <spiceNodes>1A 1B 1Y 2A 2B 2Y gnd 3Y 3A 3B 4Y 4A 4B vcc</spiceNodes>
            <rawSpice>
                .model t-nmos NMOS (level=1)
                .model t-pmos PMOS (level=1)
                .subckt NAND vcc gnd A B Y
                M1 Y A vcc vcc t-pmos
                M2 Y B vcc vcc t-pmos
                M3 Y A temp gnd t-nmos
                M4 temp B gnd gnd t-nmos
                .ends NAND
                X1 vcc gnd 1A 1B 1Y NAND
                X2 vcc gnd 2A 2B 2Y NAND
                X3 vcc gnd 3A 3B 3Y NAND
                X4 vcc gnd 4A 4B 4Y NAND
            </rawSpice>
            <datasheet>74HC00.png</datasheet>
        </icConfig>
    </part>

    <part type="ic" uid="ic-74hc02">
        <name>74HC02</name>
        <desc>This chip has quad 2-input NOR gates.</desc>
        <picture>ic.png</picture>
        <texture>ic.png</texture>
        <icConfig>
            <dipCount>14</dipCount>
            <spiceNodes>1Y 1A 1B 2Y 2A 2B gnd 3A 3B 3Y 4A 4B 4Y vcc</spiceNodes>
            <rawSpice>
                .model t-nmos NMOS (level=1)
                .model t-pmos PMOS (level=1)
                .subckt NOR vcc gnd A B Y
                M1 temp A vcc vcc t-pmos
                M2 Y B temp vcc t-pmos
                M3 Y A gnd gnd t-nmos
                M4 temp B gnd gnd t-nmos
                .ends NOR
                X1 vcc gnd 1A 1B 1Y NOR
                X2 vcc gnd 2A 2B 2Y NOR
                X3 vcc gnd 3A 3B 3Y NOR
                X4 vcc gnd 4A 4B 4Y NOR
            </rawSpice>
            <datasheet>74HC02.png</datasheet>
        </icConfig>
    </part>

    <part type="ic" uid="ic-74hc04">
        <name>74HC04</name>
        <desc>This chip has 6 hex inverters.</desc>
        <picture>ic.png</picture>
        <texture>ic.png</texture>
        <icConfig>
            <dipCount>14</dipCount>
            <spiceNodes>1A 1Y 2A 2Y 3A 3Y gnd 4Y 4A 5Y 5A 6Y 6A vcc</spiceNodes>
            <rawSpice>
                .model t-nmos NMOS (level=1)
                .model t-pmos PMOS (level=1)
                .subckt INVERT vcc gnd A Y
                M1 Y A vcc vcc t-pmos
                M2 Y A gnd gnd t-nmos
                .ends INVERT
                X1 vcc gnd 1A 1Y INVERT
                X2 vcc gnd 2A 2Y INVERT
                X3 vcc gnd 3A 3Y INVERT
                X4 vcc gnd 4A 4Y INVERT
                X5 vcc gnd 5A 5Y INVERT
                X6 vcc gnd 6A 6Y INVERT
            </rawSpice>
            <datasheet>74HC04.png</datasheet>
        </icConfig>
    </part>

    <part type="ic" uid="ic-74hc08">
