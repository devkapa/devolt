    <part type="ic" uid="ic-74hc86">
        <name>74HC86</name>
        <desc>This chip has quad 2-input XOR gates.</desc>
        <picture>ic.png</picture>
        <texture>ic.png</texture>
        <icConfig>
            <dipCount>14</dipCount>
            <spiceNodes>1A 1B 1Y 2A 2B 2Y gnd 3Y 3A 3B 4Y 4A 4B vcc</spiceNodes>
            <rawSpice>
                .model t-nmos NMOS (level=1)
                .model t-pmos PMOS (level=1)
                .subckt NAND vcc gnd A B Y
                M1 Y A vcc vcc t-pmos
                M2 Y B vcc vcc t-pmos
                M3 Y A temp gnd t-nmos
                M4 temp B gnd gnd t-nmos
                .ends NAND
                .subckt XOR vcc gnd A B Y
                X1 vcc gnd A B T NAND
                X2 vcc gnd A T G NAND
                X3 vcc gnd B T H NAND
                X4 vcc gnd G H Y NAND
                .ends XOR
                X1 vcc gnd 1A 1B 1Y XOR
                X2 vcc gnd 2A 2B 2Y XOR
                X3 vcc gnd 3A 3B 3Y XOR
                X4 vcc gnd 4A 4B 4Y XOR
            </rawSpice>
            <datasheet>74HC86.png</datasheet>
        </icConfig>
    </part>

    <part type="ic" uid="ic-74hc283">
        <name>74HC283</name>
        <desc>This chip has a 4 bit full adder with ripple carry.</desc>
        <picture>ic.png</picture>
        <texture>ic.png</texture>
        <icConfig>
            <dipCount>16</dipCount>
            <spiceNodes>S2 B2 A2 S1 A1 B1 CI gnd CO S4 B4 A4 S3 A3 B3 vcc</spiceNodes>
            <rawSpice>
                .model t-nmos NMOS (level=1)
                .model t-pmos PMOS (level=1)
                .subckt OR vcc gnd A B Y
                M1 temp A vcc vcc t-pmos
                M2 tempNOR B temp vcc t-pmos
                M3 tempNOR A gnd gnd t-nmos
                M4 tempNOR B gnd gnd t-nmos
                M5 Y tempNOR gnd gnd t-nmos
                M6 Y tempNOR vcc vcc t-pmos
                .ends OR
                .subckt AND vcc gnd A B Y
                M1 tempNAND A vcc vcc t-pmos
                M2 tempNAND B vcc vcc t-pmos
                M3 tempNAND A temp gnd t-nmos
                M4 temp B gnd gnd t-nmos
                M5 Y tempNAND gnd gnd t-nmos
                M6 Y tempNAND vcc vcc t-pmos
                .ends AND
                .subckt NAND vcc gnd A B Y
                M1 Y A vcc vcc t-pmos
                M2 Y B vcc vcc t-pmos
                M3 Y A temp gnd t-nmos
                M4 temp B gnd gnd t-nmos
                .ends NAND
                .subckt XOR vcc gnd A B Y
                X1 vcc gnd A B T NAND
                X2 vcc gnd A T G NAND
                X3 vcc gnd B T H NAND
                X4 vcc gnd G H Y NAND
                .ends XOR
                .subckt FULLADDER vcc gnd A B CIN COUT SOUT
                X1 vcc gnd A B T XOR
                X2 vcc gnd T CIN SOUT XOR
                X3 vcc gnd CIN T U AND
                X4 vcc gnd A B V AND
                X5 vcc gnd U V COUT OR
                .ends FULLADDER
                X1 vcc gnd A1 B1 CI C1 S1 FULLADDER
                X2 vcc gnd A2 B2 C1 C2 S2 FULLADDER
                X3 vcc gnd A3 B3 C2 C3 S3 FULLADDER
                X4 vcc gnd A4 B4 C3 CO S4 FULLADDER
            </rawSpice>
            <datasheet>74HC283.png</datasheet>
        </icConfig>
    </part>

